{
  "manifest_version": "0.2",
  "name": "@mokei/mcp-fetch",
  "display_name": "Mokei Fetch",
  "version": "0.4.0",
  "description": "Fetch MCP server",
  "author": {
    "name": "Paul Le Cam"
  },
  "server": {
    "type": "node",
    "entry_point": "lib/index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/lib/index.js"],
      "env": {}
    }
  },
  "tools": [
    {
      "name": "get_markdown",
      "description": "Fetch a URL and return its contents as markdown"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TairuFramework/mokei"
  }
}
