{
  "name": "mokei-repo",
  "version": "0.0.0",
  "author": "Paul Le Cam",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.2.0",
  "scripts": {
    "lint": "biome check --write ./mcp-servers ./monitor ./packages ./website",
    "test": "node --experimental-eventsource --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "test:ci": "pnpm run test --ci --runInBand",
    "build:js": "turbo run build:js",
    "build:types": "pnpm run -r build:types",
    "build:types:ci": "pnpm run -r build:types:ci",
    "build": "pnpm run build:types && pnpm run build:js",
    "build:ci": "pnpm run build:types:ci && pnpm run build:js"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "1.10.14",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "del-cli": "^6.0.0",
    "jest": "^29.7.0",
    "tsx": "^4.19.2",
    "turbo": "^2.4.0",
    "typedoc": "^0.27.6",
    "typedoc-plugin-markdown": "^4.4.1",
    "typescript": "^5.7.3"
  }
}
