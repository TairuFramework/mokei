{
  "name": "mokei",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Mokei CLI",
  "keywords": ["model", "context", "protocol", "mcp", "llm", "ai", "cli"],
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": ["/bin", "/lib", "/oclif.manifest.json"],
  "bin": {
    "mokei": "./bin/run.js"
  },
  "scripts": {
    "mokei": "./bin/dev.js",
    "build:clean": "del lib",
    "build:js": "swc src -d ./lib --config-file ../../swc.json --strip-leading-paths",
    "build": "pnpm run build:clean && pnpm run test:types && pnpm run build:js",
    "postpack": "del oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test:types": "tsc --noEmit --skipLibCheck",
    "test:unit": "OCLIF_TEST_ROOT=. node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "test": "pnpm run test:types && pnpm run test:unit",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "mokei",
    "dirname": "mokei",
    "commands": "./lib/commands",
    "plugins": ["@oclif/plugin-help", "@oclif/plugin-plugins"],
    "topicSeparator": " ",
    "topics": {}
  },
  "dependencies": {
    "@enkaku/async": "^0.10.1",
    "@enkaku/client": "^0.10.2",
    "@enkaku/node-streams-transport": "^0.10.0",
    "@enkaku/protocol": "^0.10.0",
    "@enkaku/schema": "^0.10.0",
    "@enkaku/server": "^0.10.0",
    "@enkaku/socket-transport": "^0.10.0",
    "@enkaku/stream": "^0.10.0",
    "@enkaku/transport": "^0.10.0",
    "@mokei/context-client": "workspace:^",
    "@mokei/context-protocol": "workspace:^",
    "@mokei/host": "workspace:^",
    "@mokei/host-monitor": "workspace:^",
    "@mokei/host-protocol": "workspace:^",
    "@oclif/core": "^4.2.6",
    "@oclif/plugin-help": "^6.2.24",
    "@oclif/plugin-plugins": "^5.4.30",
    "ansi-colors": "^4.1.3",
    "enquirer": "^2.4.1",
    "eventsource-parser": "^3.0.0",
    "ky": "^1.7.4",
    "nano-spawn": "^0.2.0",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.9",
    "oclif": "^4.17.25",
    "typescript": "^5.7.3"
  }
}
